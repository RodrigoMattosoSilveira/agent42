#!/bin/bash

# Usage
# agent42 function arg-2 [, arg3]

source ./feature
source ./master
source ./tag.sh

#set -ev
echo "agent42 - function: "$1
case  $1 in
  master)
    master $2
    exit $?
    ;;
  feature)
    feature $2
    exit $?
    ;;
  tag)
    tag $2 $3
    exit $?
    ;;
  *)
    echo "agent42 - Invalid function: "$1
    exit 1
    ;;
esac
